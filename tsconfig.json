{
  "compilerOptions": {
    "declaration": false,
    "incremental": true,
    "inlineSourceMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "tmp",
    "strict": true,
    "target": "ES2018",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules"
  ]
}
